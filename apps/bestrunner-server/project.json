{
  "root": "apps/bestrunner-server",
  "sourceRoot": "apps/bestrunner-server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/bestrunner-server",
        "main": "apps/bestrunner-server/src/main.ts",
        "tsConfig": "apps/bestrunner-server/tsconfig.app.json"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/bestrunner-server/src/environments/environment.ts",
              "with": "apps/bestrunner-server/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "bestrunner-server:build",
        "port": 8080
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/bestrunner-server/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/bestrunner-server"],
      "options": {
        "jestConfig": "apps/bestrunner-server/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
